{
  "name": "tenant",
  "base": "PersistedModel",
  "strict": true,
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "generated": false,
      "defaultFn": "uuidv4"
    },
    "name": {
      "type": "string",
      "required": true
    },
    "apiOwnerRole": {
      "type": "string",
      "required": true
    },
    "apiConsumerRole": {
      "type": "string",
      "required": true
    },
    "tenantAdminRole": {
      "type": "string",
      "description": "The name of the administrator role of a tenant",
      "required": true
    },
    "updatedBy": {
      "type": "string",
      "required": true
    },
    "createdBy": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "apis": {
      "type": "hasMany",
      "model": "api",
      "foreignKey": "tenantId"
    },
    "clients": {
      "type": "hasMany",
      "model": "client",
      "foreignKey": "tenantId"
    }
  },
  "acls": [],
  "methods": {},
  "mixins": {
    "TimeStamp": {
      "required" : true,
      "validateUpsert": true
    }
  }
}
